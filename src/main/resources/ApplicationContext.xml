<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation=" http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!-- bean definitions here -->
    <bean id="repoArticleBD" class="org.example.repositories.impl.ArticleRepositoryBDImpl"/>
    <bean id="repoArticleList" class="org.example.repositories.impl.ArticleRepositoryListImpl"/>
    <bean id="service" class="org.example.services.impl.CommandeServiceImpl">
        <constructor-arg ref="repoArticleList" />
    </bean>

</beans>
